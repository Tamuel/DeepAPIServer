package com.ydk.detection.domain;

import java.util.ArrayList;

public class Image {
	private final int width;
	private final int height;
	private ArrayList<ArrayList<Byte>> data;
	
	public Image(int width, int height) {
		this.width = width;
		this.height = height;
	}

	public int getWidth() {
		return width;
	}
}
