package com.ydk.detection.domain;

import java.util.ArrayList;
import javax.imageio.ImageIO;

import java.awt.image.BufferedImage;
import java.io.File;
import java.net.URL;

public class Image {
	private final int width;
	private final int height;
	private ArrayList<ArrayList<Byte>> data;
	
	public Image(int width, int height) {
		this.width = width;
		this.height = height;
		BufferedImage I = ImageIO.read(new URL(https://tamuel.github.io/assets/images/markdown-img-paste-20190310235737776.png));
		ImageIO.write(I, "png", new File("./down.png"));
	}

	public ArrayList<ArrayList<Byte>> getData() {
		return data;
	}

	public void setData(ArrayList<ArrayList<Byte>> data) {
		this.data = data;
	}

	public int getHeight() {
		return height;
	}
	
	public int getWidth() {
		return width;
	}
}
